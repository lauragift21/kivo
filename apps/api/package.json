{
  "name": "@kivo/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "npm run build -w @kivo/web && wrangler deploy",
    "db:migrate": "wrangler d1 execute kivo-db --file=./migrations/001_initial.sql",
    "db:migrate:local": "wrangler d1 execute kivo-db --local --file=./migrations/001_initial.sql",
    "db:seed": "wrangler d1 execute kivo-db --file=./migrations/002_seed.sql",
    "db:seed:local": "wrangler d1 execute kivo-db --local --file=./migrations/002_seed.sql",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@kivo/shared": "*",
    "hono": "^4.0.0",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "wrangler": "^4.54.0"
  }
}
